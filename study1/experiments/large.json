{
    "variables": {
        "$openmls"   : { "OpenmlSimulation": [1592]       , "method": "foreach" },
        "$shuffles"  : { "Shuffle"         : [100,200,300], "method": "foreach" },
        "$take"      : { "Take"            : 9000                               }
            
    },
    "simulations": [
        [ "Sector"  ,  "$shuffles", "$take" ],
        [ "Rcv1"    ,  "$shuffles", "$take" ],
        [ "$openmls",  "$shuffles", "$take" ]
    ]
}