{
    "variables": {
        "$openmls" : { "OpenmlSimulation": [137], "kwargs": { "cat_as_str":true, "take":6000 }, "method": "foreach" },
        "$shuffles": { "Shuffle"         : [100], "method": "foreach" },
        "$scale"   : { "Scale"           : ["min", "minmax"]          },
        "$cycle"   : { "Cycle"           : 15000                      }
    },
    "environments": [
        ["$openmls", "$scale", "$shuffles", ["Identity"] ]
    ]
}